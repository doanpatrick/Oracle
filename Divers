
oracle session 

avant d'exécuter vérifier

SELECT sid, serial#, username, program, status
FROM v$session 
WHERE program = 'cfmarche.exe';

BEGIN
   FOR s IN (
      SELECT sid, serial# 
      FROM v$session 
      WHERE program = 'cfmarche.exe'
        AND status = 'ACTIVE'
   )
   LOOP
      EXECUTE IMMEDIATE 'ALTER SYSTEM KILL SESSION ''' || s.sid || ',' || s.serial# || ''' IMMEDIATE';
   END LOOP;
END;
/



voir session active

SELECT 
    s.sid,
    s.serial#,
    s.username,
    s.osuser,
    s.program,
    s.machine,
    s.status,
    s.module,
    s.action,
    s.logon_time
FROM 
    v$session s
WHERE 
    s.status = 'ACTIVE'
ORDER BY 
    s.logon_time DESC;

 voir les session active avec la query


SELECT 
    s.sid,
    s.serial#,
    s.username,
    s.osuser,
    s.program,
    s.status,
    s.sql_id,
    q.sql_text,
    s.logon_time
FROM 
    v$session s
LEFT JOIN 
    v$sql q ON s.sql_id = q.sql_id
WHERE 
    s.status = 'ACTIVE'
ORDER BY 
    s.logon_time DESC;

