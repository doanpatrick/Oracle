oracle session 

avant d'exécuter vérifier

SELECT sid, serial#, username, program, status
FROM v$session 
WHERE program = 'cfmarche.exe';

BEGIN
   FOR s IN (
      SELECT sid, serial# 
      FROM v$session 
      WHERE program = 'cfmarche.exe'
        AND status = 'ACTIVE'
   )
   LOOP
      EXECUTE IMMEDIATE 'ALTER SYSTEM KILL SESSION ''' || s.sid || ',' || s.serial# || ''' IMMEDIATE';
   END LOOP;
END;
/




voir session active

select
 substr(a.spid,1,9) pid,
 substr(b.sid,1,5) sid,
 substr(b.serial#,1,5) ser#,
 substr(b.machine,1,15) box,
 substr(b.username,1,10) username,
-- b.server,
 substr(b.osuser,1,15) os_user,
 substr(b.program,1,30) program
from v$session b, v$process a
where
b.paddr = a.addr
and type='USER'
--AND b.status = 'ACTIVE'
order by username;





SELECT 
    s.sid,
    s.serial#,
    s.username,
    s.osuser,
    s.program,
    s.machine,
    s.status,
    s.module,
    s.action,
    s.logon_time
FROM 
    v$session s
WHERE 
    s.status = 'ACTIVE'
ORDER BY 
    s.logon_time DESC;

 voir les session active avec la query


SELECT 
    s.sid,
    s.serial#,
    s.username,
    s.osuser,
    s.program,
    s.status,
    s.sql_id,
    q.sql_text,
    s.logon_time
FROM 
    v$session s
LEFT JOIN 
    v$sql q ON s.sql_id = q.sql_id
WHERE 
    s.status = 'ACTIVE'
ORDER BY 
    s.logon_time DESC;

